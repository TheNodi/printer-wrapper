{
    "name": "thenodi/printer-wrapper",
    "description": "Wrapper for lp printer commands",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Nodari",
            "email": "nodarileonardo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TheNodi\\PrinterWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheNodi\\PrinterWrapper\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
