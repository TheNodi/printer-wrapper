{
    "name": "thenodi/printer-wrapper",
    "description": "Wrapper for lp printer commands",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "TheNodi\\PrinterWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheNodi\\PrinterWrapper\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
